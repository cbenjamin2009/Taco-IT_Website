{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -c tailwind.config.js -i ./app/styles/tailwind.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -c tailwind.config.js -i ./app/styles/tailwind.css -o ./app/tailwind.css --watch",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@remix-run/node": "^2.17.1",
    "@remix-run/react": "^2.17.1",
    "@remix-run/serve": "^2.17.1",
    "@remix-run/vercel": "^1.19.3",
    "concurrently": "^8.2.2",
    "isbot": "^5.1.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-typewriter": "^0.4.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.1",
    "tailwindcss": "^3.4.14"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
